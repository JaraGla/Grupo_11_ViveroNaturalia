<!-- CREATE -->
<form>
  <form>
    <div class="form-group">
      <label for="nombre">Nombre</label>
      <input
        type="text"
        class="form-control"
        id="nombre"
        aria-describedby="emailHelp"
        placeholder="Nombre"
      />
    </div>
    <div class="form-group">
      <label for="descripcion">Descripcion</label>
      <input
        type="text"
        class="form-control"
        id="descripcion"
        placeholder="Password"
      />
    </div>
    <div class="form-group">
      <label for="precio">Precio</label>
      <input
        type="text"
        class="form-control"
        id="precio"
        placeholder="Password"
      />
    </div>
    <div class="form-group">
      <label for="categoria">Categoria</label>
      <input
        type="text"
        class="form-control"
        id="categoria"
        placeholder="Password"
      />
    </div>
    <div class="form-group">
      <label for="etiqueta">Etiqueta</label>
      <input
        type="text"
        class="form-control"
        id="etiqueta"
        placeholder="Password"
      />
    </div>
    <div class="form-group">
      <label for="imagen">Imagen</label>
      <input
        type="text"
        class="form-control"
        id="imagen"
        placeholder="Password"
      />
    </div>
    <div>
      <button id="btnCreate">Crear Producto</button>
    </div>
  </form>
</form>
<!-- CREATE -->

<!-- LIST -->
<div style="margin-left: 50px">
  <% data.map(item=>{ %>

  <h5><%= item.id %></h5>
  <h1><%= item.nombre %></h1>
  <h4><%= item.precio %></h4>
  <h4><%= item.categoria %></h4>
  <h4><%= item.etiqueta %></h4>

  <% }) %>
</div>
<!-- LIST -->

<script src="/js/products.js"></script>
<%- include('../partials/footer.ejs') %>
